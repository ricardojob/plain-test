Suite UserTest {
    Properties {
        userContent: """
                     {
                         "id": 123,
                         "firstName": "John",
                         "lastName" : "Doe"
                     }"""
    }
    HTTP CreateUser {
        url   : "http://127.0.0.1:9999"
        method: "POST"
        body  : ${userContent}
        assert statusCode Equals 200
    }
}